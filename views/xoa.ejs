<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <% include css %>
    <title>Trang xóa</title>
</head>
<body class="container">
<%- include nav %>
<!-- Optional JavaScript; choose one of the two! -->
<form method="post" action="/uploadsss" enctype="application/x-www-form-urlencoded">
    <div style="margin-bottom: 200px; margin-top: 200px">
        <p class="text-center fs-2 text-uppercase" >Trang xóa</p>
        <div class="input-group mb-3">
            <input name="tenAnh" type="text" class="form-control" placeholder="Tên ảnh" aria-label="1" aria-describedby="basic-addon1">
        </div>
        <button type="submit" class="btn btn-primary">Xóa</button>
    </div>
</form>
<p><%=message%></p>
<% include footer %>

